<?xml version="1.0" encoding="utf-8"?>
<jnlp
  spec="1.0+"
  codebase="http://www.bluecove.org/bluecove-examples/bluecove-tester"
  href="awt-bctest-bluesoleil.jnlp">
  <information>
    <title>bluecove-tester (bluesoleil)</title>
    <vendor>BlueCove; Vlad Skarzhevskyy</vendor>
    <homepage href="http://www.bluecove.org/"/>
    <description>bluecove-tester (bluesoleil)</description>
    <description kind="short">bluecove-tester with BlueCove</description>
    <icon href="icon.png"/>
    <offline-allowed/>
